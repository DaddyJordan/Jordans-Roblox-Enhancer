{
	// Frame Rate Configuration
	"DFIntTaskSchedulerTargetFps": "60", // Target Frame Rate.

	// Fullscreen and Font Settings
	"DFFlagDisableDPIScale": false, // Override DPI Scaling.
	"FFlagDebugDisplayFPS": false, // Disables FPS Display.
	"FFlagHandleAltEnterFullscreenManually": false, // Manual Fullscreen Toggle.
	"FFlagPreloadAllFonts": true, // Preloads Fonts.

	// Environment Detail Settings
	"FIntFRMMinGrassDistance": "0", // Disables Grass Rendering (Min Distance).
	"FIntFRMMaxGrassDistance": "0", // Disables Grass Rendering (Max Distance).
	"FIntRenderGrassDetailStrands": "0", // Disables Grass Detail Strands.
	"FIntRenderGrassHeightScaler": "0", // Disables Grass Height Scaling.
	"FFlagGlobalWindRendering": false, // Disables Global Wind Effects.
	"FFlagGlobalWindActivated": false, // Deactivates Global Wind.

	// Texture Settings
	"DFFlagTextureQualityOverrideEnabled": true,
	"DFIntTextureQualityOverride": 3,
	"FFlagMSRefactor5": "false",

	// Sky and Visual Effects Settings
	"FFlagDebugSkyGray": true, // Activates Gray Sky.
	"FFlagDisablePostFx": true, // Disable Post-Processing Effects (Sun Beam; etc).

	// Shadow and Detail Settings
	"DFFlagDebugRenderForceTechnologyVoxel": true, // Forces Voxel Lighting.
	"DFFlagDebugPauseVoxelizer": true, // Disables Voxel's Shadows.
	"FIntRenderShadowIntensity": 0, // Disables Shadow Rendering.
	"DFIntCSGLevelOfDetailSwitchingDistance": 0, // Disables LOD Switching (General).
	"DFIntCSGLevelOfDetailSwitchingDistanceL12": 0, // Disables LOD Switching (Level 1 to 2).
	"DFIntCSGLevelOfDetailSwitchingDistanceL23": 0, // Disables LOD Switching (Level 2 to 3).
	"DFIntCSGLevelOfDetailSwitchingDistanceL34": 0, // Disables LOD Switching (Level 3 to 4).

	// Rendering and Anti-Aliasing Settings
	"FIntDebugForceMSAASamples": 0, // Disables Anti-Aliasing | MSAA.

	// Advertisement and Telemetry Settings
	"FFlagAdServiceEnabled": "false", // Disables ADs.
	"FFlagDebugDisableTelemetryEphemeralCounter": "true", // Disables Telemetry (Ephemeral Counter).
	"FFlagDebugDisableTelemetryEphemeralStat": "true", // Disables Telemetry (Ephemeral Stat).
	"FFlagDebugDisableTelemetryEventIngest": "true", // Disables Telemetry (Event Ingest).
	"FFlagDebugDisableTelemetryV2Counter": "true", // Disables Telemetry (V2 Counter).
	"FFlagDebugDisableTelemetryV2Event": "true", // Disables Telemetry (V2 Event).
	"FFlagDebugDisableTelemetryV2Stat": "true", // Disables Telemetry (V2 Stat).
	"FFlagDebugDisableTelemetryPoint": "true", // Disables Telemetry (Point).
	"FStringPerformanceSendMeasurementAPISubdomain": "opt-out", // Disables Telemetry.
	"DFStringHttpPointsReporterUrl": "http://opt-out.roblox.com", // Disables Telemetry.
	"DFStringAnalyticsEventStreamUrlEndpoint": "opt-out", // Disables Telemetry.
	"DFStringAltHttpPointsReporterUrl": "http://opt-out.roblox.com", // Disables Telemetry.
	"DFStringAltTelegrafHTTPTransportUrl": "http://opt-out.roblox.com", // Disables Telemetry.
	"DFStringTelegrafHTTPTransportUrl": "http://opt-out.roblox.com", // Disables Telemetry.
	"DFStringCrashUploadToBacktraceBaseUrl": "http://opt-out.roblox.com", // Disables Telemetry.
	"FStringErrorUploadToBacktraceBaseUrl": "http://opt-out.roblox.com", // Disables Telemetry.
	"DFStringRobloxAnalyticsSubDomain": "opt-out", // Disables Telemetry.
	"DFStringRobloxAnalyticsURL": "http://opt-out.roblox.com" // Disables Telemetry.
}
